<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<!--com.gjvandersloot.ConnectController-->
<GridPane fx:controller="com.gjvandersloot.ConnectController" hgap="10" prefHeight="369.0" prefWidth="527.0" vgap="10" xmlns:fx="http://javafx.com/fxml/1">
    <padding><Insets bottom="10" left="25" right="25" top="25" /></padding>


  <Button mnemonicParsing="false" text="Button" GridPane.columnIndex="0" GridPane.rowIndex="0" onMouseClicked="#connect" />

    <TableView fx:id="personTable" AnchorPane.bottomAnchor="10" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.topAnchor="10" GridPane.columnIndex="0" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1">
        <columns>
            <TableColumn fx:id="nameColumn" prefWidth="150" text="Name" />
            <TableColumn fx:id="emailColumn" prefWidth="200" text="Email" />
        </columns>
    </TableView>

   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
